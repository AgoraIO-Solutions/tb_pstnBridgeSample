{
    "events":
    [
        {
            "name":"Disconnected", 
            "commands":
            [
                {"stop":{}}
            ]
        },
        {
            "name":"badAudio", 
            "commands":
            [
                {"hangup":{"status":500, "reason":"Error", "reasonHeader":"api bad audio"}}
            ]
        },
        {
            "name":"Error", 
            "commands":
            [
                {"hangup":{"status":500, "reason":"Error", "reasonHeader":"api error occurred"}}
            ]
        }
    ],
    "dialogs":
    [
        {
            "name":"goodbye",
            "promptSets":
            [
                {
                    "name":"initial", 
                    "noBargeIn":
                    {
                        "prompts":
                        [
                            {"url":{"url":"goodbye.wav","maxAge":0,"maxStale":0}}
                        ]
                    }
                }
            ],
            "events":
            [
                {
                    "name":"promptsDone", 
                    "commands":
                    [
                        {"hangup":{}}
                    ]
                }
            ]
        },
        {
            "name":"acceptCall",
            "repeatCount":5,
            "timeout":10000,
            "interdigitTimeout":5000,
            "promptSets":
            [
                {
                    "name":"initial", 
                    "bargeIn":
                    {
                        "prompts":
                        [
                            {"global":{"playAs":"url","var":"welcomeUrl"}},
                            {"silence":200},
                            {"url":{"url":"joinConference.wav","maxAge":0,"maxStale":0}}
                        ]
                    }
                },
                {
                    "name":"repeat", 
                    "bargeIn":
                    {
                        "prompts":
                        [
                            {"silence":200},
                            {"url":{"url":"joinConference.wav","maxAge":0,"maxStale":0}}
                        ]
                    }
                }
            ],
            "inputs":
            [
                {
                    "DTMFOptions":
                    {
                        "options":
                        [
                            {
                                "digits":"1", 
                                "commands":
                                [
                                    {
                                        "submit":
                                        {
                                            "url":"outboundAccepted.php  ",
                                            "type":"application/x-www-form-urlencoded",
                                            "method":"GET",
                                            "fields":
                                            [
                                                {"global":{"name":"id","var":"confID"}},
                                                {"global":{"name":"partner","var":"partner"}}
                                            ]
                                        }
                                    }
                                ]
                            }
                        ]
                    }
                }
            ],
            "events":
            [
                {
                    "name":"noInput", 
                    "commands":
                    [
                        {"goto":{"dialog":"acceptCall", "promptSet":"repeat"}} 
                    ]
                },
                {
                    "name":"noMatch", 
                    "commands":
                    [
                        {"goto":{"dialog":"acceptCall", "promptSet":"repeat"}} 
                    ]
                },
                {
                    "name":"maxRepeat", 
                    "commands":
                    [
                        {"goto":{"dialog":"goodbye", "promptSet":"initial"}}
                    ]
                }
            ]
        }
    ],
    "commands":
    [
        {"goto":{"dialog":"acceptCall", "promptSet":"initial"}} 
    ]
}